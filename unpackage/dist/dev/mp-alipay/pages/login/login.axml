<view class="content"><view class="text-area"><text class="title">{{title}}</text><text>{{nameList.name}}</text><view class="_br"></view><block a:for="{{list}}" a:for-item="item" a:for-index="__i0__" a:key="id"><view>{{''+item.name+''}}</view></block><view style="width:100%;height:20px;background-color:aqua;"></view><block a:for="{{listOne}}" a:for-item="item" a:for-index="__i1__" a:key="id"><view>{{''+item.name+''}}</view></block><view style="display:flex;flex-direction:column;width:100%;"><block a:for="{{nameData}}" a:for-item="item" a:for-index="__i2__" a:key="id"><view style="width:calc(100% - rpx);height:rpx;background-color:#007AFF;"><text>{{"姓名："+item.name}}</text><button data-event-opts="{{[['tap',[['getStroage',['$event']]]]]}}" onTap="__e">你好</button></view></block></view></view></view>